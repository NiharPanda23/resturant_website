{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "build": {
      "env": {
        "NODE_VERSION": "18.x"
        }
    },
    "engines": {
      "node": ">=18.0.0"
    },
    "installCommand": "npm ci"
  }